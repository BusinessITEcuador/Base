<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>hexagonal.api</Name>
    <Description>proyecto base hexagonal.api</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>hexagonal.api</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <CreateInPlace>true</CreateInPlace>
    <Icon>__TemplateIcon.ico</Icon>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="hexagonal.api.csproj" File="hexagonal.api.csproj" ReplaceParameters="true">
      <Folder Name="Properties" TargetFolderName="Properties">
        <ProjectItem ReplaceParameters="true" TargetFileName="launchSettings.json">launchSettings.json</ProjectItem>
      </Folder>
      <Folder Name="Controllers" TargetFolderName="Controllers">
        <Folder Name="bases" TargetFolderName="bases">
          <ProjectItem ReplaceParameters="true" TargetFileName="APIControllerBase.cs">APIControllerBase.cs</ProjectItem>
        </Folder>
        <ProjectItem ReplaceParameters="true" TargetFileName="PersonController.cs">PersonController.cs</ProjectItem>
      </Folder>
      <Folder Name="enums" TargetFolderName="enums">
        <ProjectItem ReplaceParameters="true" TargetFileName="ResponseCode.cs">ResponseCode.cs</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="ResponseCodeText.cs">ResponseCodeText.cs</ProjectItem>
      </Folder>
      <Folder Name="extensions" TargetFolderName="extensions">
        <Folder Name="automappers" TargetFolderName="automappers">
          <ProjectItem ReplaceParameters="true" TargetFileName="AutoMapperExtension.cs">AutoMapperExtension.cs</ProjectItem>
        </Folder>
        <Folder Name="injections" TargetFolderName="injections">
          <ProjectItem ReplaceParameters="true" TargetFileName="DependencyInjectionExtension.cs">DependencyInjectionExtension.cs</ProjectItem>
        </Folder>
        <Folder Name="servers" TargetFolderName="servers">
          <ProjectItem ReplaceParameters="true" TargetFileName="ServerExtension.cs">ServerExtension.cs</ProjectItem>
        </Folder>
      </Folder>
      <Folder Name="models" TargetFolderName="models">
        <ProjectItem ReplaceParameters="true" TargetFileName="ResponseBase.cs">ResponseBase.cs</ProjectItem>
      </Folder>
      <ProjectItem ReplaceParameters="true" TargetFileName="appsettings.json">appsettings.json</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="appsettings.Development.json">appsettings.Development.json</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="Program.cs">Program.cs</ProjectItem>
    </Project>
  </TemplateContent>
</VSTemplate>