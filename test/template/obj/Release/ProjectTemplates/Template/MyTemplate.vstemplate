<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="ProjectGroup">
  <TemplateData>
    <Name>hexagonal</Name>
    <Description>Prueba hexagonal</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>hexagonal</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <CreateInPlace>true</CreateInPlace>
    <Icon>__TemplateIcon.png</Icon>
    <PreviewImage>__PreviewImage.png</PreviewImage>
  </TemplateData>
  <TemplateContent>
  <ProjectCollection>
    <ProjectTemplateLink ProjectName="$projectname$.hexagonal.api">
    src\hexagonal.api\MyTemplate.vstemplate
    </ProjectTemplateLink>
    <ProjectTemplateLink ProjectName="$projectname$.hexagonal.data">
    src\hexagonal.data\MyTemplate.vstemplate
    </ProjectTemplateLink>
    <ProjectTemplateLink ProjectName="$projectname$.hexagonal.domain">
    src\hexagonal.domain\MyTemplate.vstemplate
    </ProjectTemplateLink>
    <ProjectTemplateLink ProjectName="$projectname$.hexagonal.application">
    src\hexagonal.application\MyTemplate.vstemplate
    </ProjectTemplateLink>
  </ProjectCollection>
  </TemplateContent>
</VSTemplate>